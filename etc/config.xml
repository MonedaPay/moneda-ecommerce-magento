<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <moneda_pay>
                <active>0</active>
                <model>Ari10\MonedaPay\Model\Methods\MonedaPay</model>
                <environment>app</environment>
                <order_status>pending_payment</order_status>
                <title>MonedaPay</title>
                <private_key backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <debug>0</debug>
                <allowspecific>0</allowspecific>
                <can_use_internal>1</can_use_internal>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>0</is_gateway>
                <is_offline>0</is_offline>
                <authorize>0</authorize>
                <capture>0</capture>
                <void>0</void>
                <can_initialize>1</can_initialize>
                <sort_order>100</sort_order>
            </moneda_pay>
        </payment>
    </default>
</config>
